#include<iostream>
#include<algorithm>

using namespace std;
int main(){
    int a[6][6],row,col;
    for(int i=1;i<=5;i++){
        for(int j=1;j<=5;j++){
            cin>>a[i][j];
            if(a[i][j]==1){
                row=i;
                col=j;
              break;
    }
}

}
 
cout<<(abs(row-3))+ abs((col-3))<<endl;
return 0;
}
 
