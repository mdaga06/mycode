#include<iostream>
using namespace std;
int main()
{
    int j=0,c=0;
    string s,b="hello";
    cin>>s;
    int n=s.size();
   if(n>5){
      for(int i=0;i<n;i++){
          if(s[i]==b[j]){
          j++;
          c++;
      }
      if(c==5)
      break;
  }
   }
  if(c==5 || s=="hello"){
      cout<<"YES"<<endl;
  }
  else{
      cout<<"NO"<<endl;
  }
  
    return 0;
}
