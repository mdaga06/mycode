#include<iostream>
#include <string>
using namespace std;
int main(){
   string a,b;
   cin>>a>>b;
   for(size_t i=0;i<a.length();i++){
       if(a[i]==b[i]){
           a[i]='0';
       }
        else if(a[i]!=b[i]){
           a[i]='1';
       }
   }
    for(int i=0;i<a.length();i++){
        cout<<a[i];
    }
   return 0;
}
